@page
@model ResgistroUsuario.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Detalle del Usuario</h1>

<div>
    <h4>Registros</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RegistroU.Nombre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegistroU.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RegistroU.Apellidos)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegistroU.Apellidos)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RegistroU.Usuario)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegistroU.Usuario)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RegistroU.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegistroU.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RegistroU.Password)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegistroU.Password)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.RegistroU.ID">Modificar</a> |
    <a asp-page="./Index">Volver a la Lista</a>
</div>
